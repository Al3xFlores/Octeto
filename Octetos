SELECT 
  coluna1,
  coluna2,
  coluna3,
  SUBSTR(ip_column, 1, INSTR(ip_column, '.', 1, 3) - 1) AS first_three_octets,
  (SELECT COUNT(*)
   FROM your_table AS subquery
   WHERE SUBSTR(subquery.ip_column, 1, INSTR(subquery.ip_column, '.', 1, 3) - 1) = 
         SUBSTR(your_table.ip_column, 1, INSTR(your_table.ip_column, '.', 1, 3) - 1)
  ) AS count
FROM your_table;

